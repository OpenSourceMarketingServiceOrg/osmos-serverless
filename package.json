{
  "name": "osmose-serverless",
  "version": "0.1.0",
  "description": "OSMoSE serverlesss backend written for AWS Lambda",
  "main": "handler.js",
  "author": "OpenSourceMarketingService <OpenSourceMarketingService@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenSourceMarketingServiceOrg/osmos-serverless.git"
  },
  "keywords": [
    "lambda",
    "serverless",
    "OSMoSE",
    "SES",
    "aws",
    "SNS",
    "DynamoDB",
    "email"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run f:ses && npm run f:api",
    "f:api": "serverless invoke local -f emailList -p api_event.json",
    "f:ses": "serverless invoke local -f saveEmailStatus -p event.json"
  },
  "devDependencies": {
    "serverless": "^1.17.0",
    "serverless-offline": "^3.15.1"
  },
  "dependencies": {
    "aws-sdk": "^2.122.0"
  }
}
